        <co:prefLabel xml:lang="en">Photography</co:prefLabel>
        <co:scopeNote xml:lang="en">Used for all coverage of photographers and photography, including fine art, commercial, news (photojournalism) and recreational photography.&#60;br&#62;</co:scopeNote>
    </co:Concept>

